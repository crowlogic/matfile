#!/bin/sh -x

BASEDIR=$(dirname $(readlink -f $0))/..
cd $BASEDIR
swig -v -java -package arblib -outdir src/arblib -I/usr/include/x86_64-linux-gnu -I/usr/include/linux -I/usr/include/flint -I/usr/include -I/usr/include/i386-linux-gnu -I/usr/lib/gcc/x86_64-linux-gnu/11/include/ arb.i
$BASEDIR/bin/caw

